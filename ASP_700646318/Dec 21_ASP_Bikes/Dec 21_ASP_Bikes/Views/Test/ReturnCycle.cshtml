@model Dec_21_ASP_Bikes.Models.CycleRequestedByUser


@using (Html.BeginForm())
{

    <div>
        <h4>CycleRequestedByUser</h4>

        @if (ViewBag.Error != null)
        {
            <h2>@ViewBag.Error</h2>
        }
        else

        {



            <hr />
            <dl class="dl-horizontal">
                <dt>
                    @Html.DisplayNameFor(model => model.CycleID)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.CycleID)
                </dd>


               

               

                <dt>
                    @Html.DisplayNameFor(model => model.RequestID)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.RequestID)
                </dd>

            </dl>

            <div class="form-group">
                <div class="col-md-offset-2 col-md-10">
                    <input type="submit" value="Return Cycle" class="btn btn-default" />
                </div>
            </div>

        }

     



    </div>


}

    







